    {% for message in app.flashes('failed') %}
        <div class="alert alert-danger mt-2" role="alert">
            {{ message }}
        </div>
    {% endfor %}
    <div class="w-75 mt-5 mx-auto">
{#        {% include 'event/_event_filter.html.twig' %}#}
        {% include 'event/_form.html.twig' %}
        {% include 'event/_liste.html.twig' %}
    </div>

{% endblock %}

